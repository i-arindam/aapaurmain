}
li.dropdown a.link-profile {
  overflow: visible;
}
li.dropdown a.link-profile img {
  border: 1px solid #CCC;
  padding: 3px;
  border-radius: 4px;
  margin-top: -11px;
}
.header .nav ul.dropdown-menu {
  width: 190px;
}
.log-in {
  margin-top: -12px;
  position: relative;
}
.log-in a.btn {
  padding: 10px 32px;
  -webkit-transition: none !important;
  -moz-transition: none !important;
  transition: none !important;
}
.log-in:hover .login-form-container {
  -webkit-animation: translate-in 0.5s; /* Safari 4+ */
  animation: translate-in 0.5s;
  display: block;
  opacity: 1;
}
@-webkit-keyframes translate-in {
  0% {
    opacity: 0;
    -webkit-transform: translateX(10px);
    transform: translateX(10px);
  }
  100% {
   opacity: 1;
    -webkit-transform: translateX(0);;
    transform: translateX(0);
  }
}
@keyframes translate-in {
  0% {
    opacity: 0;
    -webkit-transform: translateX(10px);
    transform: translateX(10px);
  }
  100% {
   opacity: 1;
    -webkit-transform: translateX(0);;
    transform: translateX(0);
  }
}

