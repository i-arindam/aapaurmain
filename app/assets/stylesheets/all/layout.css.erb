}
.tags a:after{
  content:"";
  position:absolute;
  top:13px;
  left:9px;
  float:left;
  width:6px;
  height:6px;
  -moz-border-radius:50%;
  -webkit-border-radius:50%;
  border-radius:50%;
  background:#fff;
  -moz-box-shadow:-1px -1px 2px #004977;
  -webkit-box-shadow:-1px -1px 2px #004977;
  box-shadow:-1px -1px 2px #004977;
}
.tags a.selected {
  background: green;
}
.tags a:hover:before{
  border-color:transparent #555 transparent transparent;
}
