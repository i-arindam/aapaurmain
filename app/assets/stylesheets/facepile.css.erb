.facepile {
  /*background: #E9E8E8;*/
  border-radius: 10px;
  top: 110px;
  position: relative;
}
.header-wrap{
  background: rgba(0, 0, 0, 0.7);
  color: #FFF;
  padding: 12px 58px 12px 15px;
  border-radius: 6px 6px 0 0;
  -webkit-border-radius: 6px 6px 0 0;
  -moz-border-radius: 6px 6px 0 0;
  -ms-border-radius: 6px 6px 0 0;
}
.facepile-header {
  line-height: 1.5em;
  font-size: 16px;
}
ul.user-faces {
  overflow: hidden;
  padding-top: 5px;
}
li.user-face {
  float: left;
  width: 60px;
  height: 60px;
  border: 5px inset #82D8D4;
  margin: 2px;
  border-radius: 8px;
  -webkit-border-radius: 8px;
  -moz-border-radius: 8px;
  -ms-border-radius: 8px;
}
li.user-face img {
  border-radius: 8px;
  -webkit-border-radius: 8px;
  -moz-border-radius: 8px;
  -ms-border-radius: 8px;
  width: 60px;
  height: 60px;
}
.users-container {
  padding: 10px;
  background: #E9E8E8;
  border-radius: 0 0 6px 6px;
  -webkit-border-radius: 0 0 6px 6px;
  -moz-border-radius: 0 0 6px 6px;
  -ms-border-radius: 0 0 6px 6px;  
}
.panel-name {
  margin-bottom: 5px;
  font-size: 15px;
  line-height: 20px;
}
.panel-users {
  padding: 10px 0;
  border-bottom: 1px solid #000;
}
.panel-users:last-child {
  border-bottom: none;
}
span.panel {
  background-color:#f2dede;
  border-color:#eed3d7;
  color:#b94a48;
  padding: 5px;
}
