/*Social Actions*/


.socialActions {
  background: rgba(187, 78, 44, 0.74);
  border: 1px solid #555;
  -webkit-border-radius: 5px;
  -moz-border-radius: 5px;
  -ms-border-radius: 5px;
  border-radius: 5px;
  left: 55px;
  list-style: none;
  margin: 0;
  padding: 15px 10px;
  position: absolute;
  width: 60px;
  top:245px;
}

.socialActions li {
  color: #333;
  float: right;
  line-height: 1;
  margin: 0 0 15px;
  vertical-align: top;
}

.socialActions .social-fb {
  margin-right: 6px;
}

.socialActions .social-fb .fb-like > span {
  width: 48px !important;
}

.socialActions .social-tw {
  margin-right: 2px;
}

.socialActions .social-li {
  margin-top: -11px;
}

.socialActions .social-go {
  margin-right: 4px;
}

.socialActions .social-go > div {
  height: 20px !important;
}

.socialActions .social-pi {
  margin-bottom: 0;
  margin-right: 8px;
}

.socialActions li div,
.socialActions li iframe {
  vertical-align: top !important;
}

.socialActions .fb-like {
  float: left !important;
}

.IN-widget span,
span.IN-widget span:first-child {
  vertical-align: top !important;
}
/* Social Actions End*/



.faqs {
  color: #D3D3D3;
  top: 300px;
  margin-top: 80px;
  background: #191814 url('<%= image_path "faq_bg.png" %>')
}

h2.faqHeader {
  padding-top: 20px;
  padding-bottom: 20px;
  margin-bottom: 0px;
  font-weight: bold;
  font-family: 'Oregano' cursive;
}

.faqWrapper {
  height: 500px;
  margin-left: auto;
  margin-right: auto;
  width: 980px;
}

.leftCol {
  margin-right: 10px;
}

.rightCol {
  margin-left: 10px;
}

.leftCol, .rightCol {
  float: left;
  background: rgba(36, 34, 82, 0.51);
  border-radius: 10px;
  width: 450px;
  height: 450px;
}

.leftCol .question, .rightCol .question {
  font-family: 'Arial' cursive;
  font-size: 26px;
  position: relative;
  color: #DBC896;
  line-height: 24px;
  text-align: center;
  margin-top: 20px;
}

.leftCol p, .rightCol p {
  font-family: 'Times New Roman' sans-serif;
  font-size: 16px;
  position: relative;
  line-height: 19px;
}

.q {
  padding: 10px;
}

.whatWeAre {
  font-size: 15px;  
  margin-top: 26px;
  font-family: Georgia, "Times New Roman", Times, serif;
  font-style: italic;
}

.whatWeAre li {
  padding-bottom: 15px;
  line-height: 26px;
  word-spacing: 1px;
}

.booklet div.b-wrap-right{
  background: url('<%= image_path "booklet/book-part2.png" %>') no-repeat top left;
}

.booklet div.b-wrap-left{
  background: url('<%= image_path "booklet/book-part1.png" %>') no-repeat top left; 
}

