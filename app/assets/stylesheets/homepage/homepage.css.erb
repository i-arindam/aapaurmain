.signup {
  clear: both; 
  margin-right: 76px;
  position: relative;
  float: right;
  width: 311px;
  height: 200px;
  top:-24px;
}
.signup img{
  margin-top: 78px;
  margin-left: 17px;
}

#signup_form {
  position: relative;
  left: 8px;
  top: 100px;
}

.signupBtn {
  background:url('<%= image_path "arrow.png" %>') no-repeat;
  width: 30px;
  height: 30px;
  margin-left: 200px;
  background-color: whiteSmoke;
}

.input-prepend span.add-on {
  margin-right: -5px;
}

.signupError {
  float: right;
  position: relative;
  right: 60px;
  width: 310px;
  top: 30px;
  list-style: none;
  list-style-type: none;
}

.signupError ul {
  list-style-type: none;
  padding-top: 10px;
}

.signupError h2 {
  margin-bottom: 2px;
}

.stickyArea {
  position: absolute;
  left: 200px;
  top: 150px;
  padding: 10px;
  width: 560px;
  -webkit-border-radius: 10px;
  -moz-border-radius: 10px;
  -ms-border-radius: 10px;
  -o-border-radius: 10px;
  border-radius: 10px;
}

.fmascot {
  display:none;
  position: absolute;
  height: 1000px;
  width: 780px;
  left: -220px;
  top: 344px;
}


.mmascot {
  display:none;
  position: absolute;
  left: 600px;
}

.calloutCloud {
  display:none;
  position: relative;
  left: 310px;
}


