/* content layout */
.container {
  margin: 0;
  width: 100%;
}
.row-fluid .span9 {
  width: 78.4%;
}
.row-fluid .span3 {
  width: 21.6%;
  margin: 0;
}
.col-one {
  float: left;
  width: 25.5%;
  padding: 77px 0 0 0;
}
.col-two {
  margin-left: 25.5%;
  border-left: 2px solid #ededed;
  background: #f9f7f8;
  overflow: hidden;
}
/* layout Ends */

.center-container {
  padding: 16px 32px;
}

/* Feeds */
.feeds-container {
  margin: 61px 0 30px 50px;
}
.feeds-wrap {
  background: #f5f4f5;
  border: 1px solid #cfcfcf;
  padding: 21px 33px;
  border-radius: 3px;
  -moz-border-radius: 3px;
  -webkit-border-radius: 3px;
  width: 90%;
  position: relative;
  margin: 0 0 30px;
}
.feeds-box {
  width: 96%;
  height: 30px;
  background: #fff;
  padding: 7px 12px;
}
.feeds-lbl {
  color: #B3B3B3;
  font-size: 13px;
  left: 48px;
  position: absolute;
  top: 31px;
}
.feed-btns a {
  float: left;
  margin: 0 2.5%;
  width: 27.4%;
}
.btn span.active-feed {
  background-position: 7px -662px;
}
.btn span.short-feed {
  background-position: 7px -694px;
}
.btn span.loc-feed {
  background-position: 7px -725px;
}
.qa-content {
  padding: 77px 20px 0;
}
/* Feeds Ends */

.no-content {
  min-height: 200px;
  /*padding-top: 20px;*/
  font-size: 14px;
}

.rs-wrap {
  padding: 30px;
  background: #C2C74D;
}

/* slider overrides */
.rs-prev.disable-action, .rs-next.disable-action {
  pointer-events: none;
}
/* slider overrides */

.hero-unit {
  text-align: center;
}
.hero-unit .sorry-message {
  font-size: 28px;
}
