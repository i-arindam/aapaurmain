/* content layout */
.container {
  margin: 0;
  width: 100%;
}
.row-fluid .span9 {
  width: 78.4%;
}
.row-fluid .span3 {
  width: 21.6%;
  margin: 0;
}
.col-one {
  float: left;
  width: 25.5%;
  padding: 77px 0 0 0;
}
.col-two {
  margin-left: 25.5%;
  border-left: 2px solid #ededed;
  background: #f9f7f8;
  overflow: hidden;
}
/* layout Ends */

/* right bar */
.rit-content {
  padding: 77px 20px 0;
}
.tit-wrap {
  background: #e9e8e8;
  border-bottom: 1px solid #cac9c9;
  padding: 12px 8px;
  margin: 0 0 13px;
}
.tit-wrap h2 {
  color: #444242;
  font-size: 16px;
}
.board-tit {
  background: url("<%= image_path 'sprite_layout.png' %>") no-repeat 0 -570px;
  padding: 7px 0 7px 37px;
}
.board-wrap {
  margin: 0 0 28px;
}
.board-wrap ul {
  margin: 0 0 0 47px;
}
.board-wrap ul li {
  margin: 0 0 16px 0;
}
.datas {
  font-size: 13px;
  color: #696a68;
}
.shared-by {
  color: #434343;
  font-size: 13px;
  font-family: 'lucida_sansdemibold_roman';
  margin: 0 0 10px;
}
.user-left {
  float: left;
  margin: 0 9px 0 0;
  width: 32px;
}
.user-left img, .comment-user img {
  width: 100%;
  border-radius: 3px;
  -moz-border-radius: 3px;
  -webkit-border-radius: 3px;
}
.qn-right {
  float: left;
  width: 77%;
}
.qa-wrap ul li {
  overflow: hidden;
  margin: 0 0 20px;
}
.qa-wrap {
  position: relative;
}
.new-label {
  position: absolute;
  right: 0;
  top: -1px;
}
.questions-loader, .panels-loader, .top-stories-loader {
  min-height: 240px;
  padding: 10px;
}
/* right bar ends */

.center-container {
  padding: 16px 32px;
}


/* Feeds */
.feeds-container {
  margin: 61px 0 30px 50px;
}
.feeds-wrap {
  background: #f5f4f5;
  border: 1px solid #cfcfcf;
  padding: 21px 33px;
  border-radius: 3px;
  -moz-border-radius: 3px;
  -webkit-border-radius: 3px;
  width: 90%;
  position: relative;
  margin: 0 0 30px;
}
.feeds-box {
  width: 96%;
  height: 30px;
  background: #fff;
  padding: 7px 12px;
}
.feeds-lbl {
  color: #B3B3B3;
  font-size: 13px;
  left: 48px;
  position: absolute;
  top: 31px;
}
.feed-btns a {
  float: left;
  margin: 0 2.5%;
  width: 27.4%;
}
.btn span.active-feed {
  background-position: 7px -662px;
}
.btn span.short-feed {
  background-position: 7px -694px;
}
.btn span.loc-feed {
  background-position: 7px -725px;
}
.qa-content {
  padding: 77px 20px 0;
}
/* Feeds Ends */

.no-content {
  min-height: 200px;
  /*padding-top: 20px;*/
  font-size: 14px;
}
