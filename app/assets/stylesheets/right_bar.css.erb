.qn-right {
  float: left;
  width: 77%;
}
.user-left {
  float: left;
  margin: 0 9px 0 0;
  width: 32px;
}
.user-left img {
  border-radius: 3px;
  -moz-border-radius: 3px;
  -webkit-border-radius: 3px;
}
.datas {
  font-size: 13px;
  color: #696a68;
}
.shared-by {
  color: #434343;
  font-size: 13px;
  font-family: 'lucida_sansdemibold_roman';
  margin: 0 0 10px;
}
.rit-content {
  padding: 77px 20px 0;
}
.tit-wrap {
  background: rgba(0, 0, 0, 0.7);
  color: #fff;
  padding: 12px 8px;
  border-radius: 6px 6px 0 0;
  -webkit-border-radius: 6px 6px 0 0;
  -moz-border-radius: 6px 6px 0 0;
  -ms-border-radius: 6px 6px 0 0;
  -o-border-radius: 6px 6px 0 0;
}
.tit-wrap h2 {
  font-size: 16px;
}
.board-tit {
  background: url("<%= image_path 'sprite_layout.png' %>") no-repeat 0 -570px;
  padding: 7px 0 7px 37px;
}
.board-wrap {
  margin: 0 0 28px;
}
.board-wrap ul li {
  margin: 0 0 16px 0;
}
.qa-wrap ul li {
  overflow: hidden;
  margin: 0 0 20px;
}
.qa-wrap {
  position: relative;
}
.new-label {
  position: absolute;
  right: 0;
  top: -1px;
}
.questions-loader, .panels-loader, .top-stories-loader {
  min-height: 240px;
  padding: 10px;
}
ul.panels, ul.questions {
  padding: 10px;
  background: #e9e8e8;
  border-radius: 0 0 6px 6px;
  -webkit-border-radius: 0 0 6px 6px;
  -moz-border-radius: 0 0 6px 6px;
  -ms-border-radius: 0 0 6px 6px;
  -o-border-radius: 0 0 6px 6px;
}
.rit-content ul.story-tags {
  padding-left: 0;
}
