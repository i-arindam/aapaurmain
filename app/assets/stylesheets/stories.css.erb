/* Story */
.story-container li {
  margin: 0 0 30px;
}
.social-share {
  display: inline;
  position: relative;
  top: 150px;
}
li.story {
  -webkit-transition: all 0.2s ease-in;
  -moz-transition: all 0.2s ease-in;
  -o-transition: all 0.2s ease-in;
  margin-bottom: 10px;
}
li.story:hover {
  background: rgba(0, 0, 0, 0.7);
  box-shadow: 9px 9px 12px #777;
  -moz-border-radius: 5px;
  -webkit-border-radius: 5px;
  -ms-border-radius: 5px;
  border-radius: 5px;
}
.story-container li:after {
  content: ".";
  display: block;
  clear: both;
  visibility: hidden;
  line-height: 0;
  height: 0;
}
.story-user-wrap {
  float: left;
  width: 35px;
  margin: 0 16px 0 0;
}
.story-user img {
  width: 100%;
}
.story-content {
  float: left;
  width: 92.5%;
}
.story-tit {
  position: relative;
  background: #dcdbda;
}
.arrow-left {
  background: url("<%= image_path 'sprite_layout.png' %>") no-repeat scroll 0 -628px;
  height: 21px;
  position: absolute;
  left: -10px;
  top: 12px;
  width: 18px;
}
.paper-roll {
  background: url("<%= image_path 'sprite_layout.png' %>") no-repeat scroll -48px 0;
  height: 21px;
  position: absolute;
  right: 0;
  top: 0;
  width: 18px;
}
.story-tit {
  padding: 16px 25px;
  -moz-border-radius: 5px 29px 0 0;
  -webkit-border-radius: 5px 29px 0 0;
  border-radius: 5px 29px 0 0;
}
.story-tit h3 {
  color: #444242;
  font-size: 16px;
  float: left;
}
.story-time a.story-link {
  color: #000;
}
.del-story {
  float: none;
  margin-left: 15px;
}
.story-tit p {
  color: #7c7c7c;
  font-size: 12px;
  float: right;
}
.story-inner {
  background: #f0f0ef;
  -moz-border-radius: 0 0 8px 8px;
  -webkit-border-radius: 0 0 8px 8px;
  border-radius: 0 0 8px 8px;
}
.story {
  padding: 12px 0 25px 0;
  overflow: hidden;
  list-style-type: none;
}
.story-img {
  float: left;
  width: 87px;
  margin: 0 14px 0 0;
}
.story-img img {
  width: 100%;
}
.story-text {
  color: #000;
  float: left;
  font-size: 14px;
  padding: 0 12px 0 12px;
  /*width: 100%;*/
}
.like-area {
  background: #e4e4e3;
  padding: 10px 25px;
  color: #7c7c7c;
}
.like-area span {
  display: block;
  float: left;
  padding: 6px 22px 0 7px;
}
.like-area a.hover-scale {
  -webkit-transition: all 0.2s ease-in;
  -moz-transition: all 0.2s ease-in;
  -o-transition: all 0.2s ease-in;  
}
.like-area a.hover-scale:hover {
  -webkit-transform: scale3d(1.5, 1.5, 1.5);
  -moz-transform: scale3d(1.5, 1.5, 1.5);
  -o-transform: scale3d(1.5, 1.5, 1.5);
}
.link-like, .link-dislike, .comment-count {
  float: left;
  background: url("<%= image_path 'sprite_layout.png' %>") no-repeat 0 0;
  display: block;
}
.story-boos:hover, .story-claps:hover, .story-comments:hover {
  text-decoration: underline;
}
.comment-count {
  background-position: -48px -24px;
  padding: 5px 0 2px 25px;
  height: 21px;
}
.comment-count-wrap {
  float: right;
}
.link-like, .link-like-comment {
  background-position: -50px -52px;
  height: 23px;
  width: 18px;
}
.link-dislike, .link-dislike-comment {
  background-position: -50px -84px;
  height: 23px;
  width: 18px;
}
.comment-area {
  padding: 17px 25px;
}
.link-like-comment, .link-dislike-comment {
  background-image: url("<%= image_path 'sprite_layout.png' %>");
  display: block;
}
.comment-link {
  background: url("<%= image_path 'users/arrow-sprite.png' %>") no-repeat scroll right 5px;
  padding: 0 25px 0 0;
  color: #444242;
  font-size: 14px;
  font-family: 'lucida_sansdemibold_roman';
}
.comment-link.active {
  background-position: right -8px;
}
.comment-area ul {
  margin: 10px 0 0 0
}
.comment-area li {
  margin: 0 0 10px;
  padding-bottom: 10px;
  border-bottom: 1px #000 dotted;
}
.comment-area li:last-child {
  border-bottom: none;
}
.comment-user {
  float: left;
  margin: 0 17px 0 0;
  width: 48px;
}
.comment-user img {
  width: 35px;
  border-radius: 3px;
  -moz-border-radius: 3px;
  -webkit-border-radius: 3px;
}
.comment {
  float: left;
  width: 89%;
}
.comment-prsn {
  margin: 0 0 10px;
}
.comment-prsn p {
  font-size: 14px;
  float: left;
}
.comment-prsn span {
  font-size: 12px;
  color: #969695;
}
.del-comment {
  float: none;
}
.comment-wrap {
  position: relative;
}
.comment-box, .feeds-box {
  background: #f8f8f8;
  border: 1px solid #c9c9c6;
  border-radius: 3px;
  -webkit-border-radius: 3px;
  -moz-border-radius: 3px;
  -ms-border-radius: 3px;
  -o-border-radius: 3px;
  box-shadow: none;
  color: #000;
  font-size: 13px;
  resize: none;
  width: 95%;
  margin: 0;
  padding: 12px;
}
.comment-box:focus, .feeds-box:focus {
  border-radius: none;
  box-shadow: none;
  border: 1px solid #c9c9c6;
}
.comment-actions {
  padding: 10px;
  position: relative;
  left: 80%;
}
.comment-actions .cancel-comment {
  text-decoration: none;
}
.cmt-lbl {
  position: absolute;
  top: 13px;
  left: 12px;
  color: #b3b3b3;
  font-size: 13px;
}
.comment-box {
  height: 34px;
}
.comment-text {
  width: 80%;
  float: left;
  color: #000;
}
.comment-like-area {
  float: right;
}
.comment-like-area a {
  float: left;
}
.comment-like-area a[class*=j-] {
  margin: 4px 12px 0 5px;
}
.panels-area{
  padding-top: 10px;
}

/* tags */
ul.story-tags{
  margin-bottom: 5px;
  padding-left: 20px;
  list-style:none;
  overflow: hidden;
}
.story-tags li, .story-tags a{
  float: left;
  height: 14px;
  line-height: 10px;
  position: relative;
  font-size: 10px;
  margin-bottom: 10px;
}
.story-tags a{
  margin-left: 5px;
  padding: 6px 6px 1px 6px;
  background: #5F686D;
  color: #fff;
  text-decoration: none;
  -moz-border-radius: 4px;
  -webkit-border-radius: 4px;
  border-radius: 4px;
  text-transform: uppercase;
}
.story-tags a:hover{
  background: #185F18;
  -moz-box-shadow: 5px 5px 7px rgba(33,33,33,1);
  -webkit-box-shadow: 5px 5px 7px rgba(33,33,33,.7);
  box-shadow: 5px 5px 7px rgba(33,33,33,.7);
  -moz-transition: all 0.2s linear;
  -webkit-transition: all 0.2s ease-in;
  -ms-transition: all 0.2s ease-in;
  -o-transition: all 0.2s ease-in;
  transition: all 0.2s ease-in;
  z-index: 5;
}
.more-stories-container {
  text-align: center;
}
.more-stories-container .btn {
  margin-left: 34px;
}
/* Story end */

/* popup styles */
.people-list a {
  font-size: 20px;
  line-height: 20px;
}
.people-list img {
  margin-right: 20px;
  width: 60px;
  height: 60px;
  padding: 4px;
  border: 1px solid #2CDFD0;
  margin: 2px;
  border-radius: 8px;
  -webkit-border-radius: 8px;
  -moz-border-radius: 8px;
  -ms-border-radius: 8px;  
}
.people-list li {
  border-bottom: 1px dotted #423F3F;
  padding-bottom: 10px;
  margin-top: 10px;
}
.people-list li:last-child {
  border-bottom: none;
}
.people-list li:first-child {
  margin-top: 0;
}
/* one-story */
.one-story .center-container {
  height: 100%;
}
.one-story li.story {
  position: relative;
}
div.embed {
  border: 2px outset;
  border-radius: 10px;
  padding: 15px;
  background: #DFDFDC;
  width: 95%;
  margin: 10px 0 10px 0;
  overflow: hidden;
}
div.embed img {
  float: left;
  padding-right: 30px;
}
div.embed .provider {
  display: none;
}
div.embed a {
  display: block;
}
div.embed .description {
  padding-top: 16px;
}
/* End of one-story */
.usingFallback {
  background: #41B6E4;
  padding: 10px;
  font-size: 16px;
  -moz-border-radius: 5px;
  -webkit-border-radius: 5px;
  -ms-border-radius: 5px;
  border-radius: 5px;
}
.close_popup {
  position: absolute;
  top: 0px;
  right: 10px;
  border: 3px solid #000;
  border-radius: 50%;
  padding: 0px 2px 2px 2px;  
}
.disabled-link {
  pointer-events: none;
  color: #000;
}


/* One story signup */
.one-story.container {
  margin-bottom: 20px;
}
.signup-form {
  width: 470px;
  margin: 0 15px 0 0;
}
.signup-form h3 {
  border-bottom: 1px solid rgba(255, 255, 255, 0.54);
  color: #fff;
  float: right;
  font-family: 'voltaireregular', helvetica neue, arial, sans-serif;
  font-size: 20px;
  margin: 0 0 27px;
  padding: 0 0 19px;
  text-align: center;
  text-shadow: 0 1px 1px #000;
  width: 455px;
}
.signup-form input[type=text], 
.signup-form input[type=password] {
  background: #fff;
  border: 0;
  box-shadow: 0 1px 1px #555;
  height: 30px;
  margin: 0 0 20px 15px;
  padding-left: 11px;
  width: 203px;
}
.conditions {
  width: 455px;
  float: right;
  margin: 0px 0 15px;
  text-shadow: 0 1px 1px rgba(0, 0, 0, 0.43);
}
.conditions label {
  color: #fff;
  cursor: pointer;
  font-family: 'voltaireregular', sans-serif;
  font-size: 15px;
}
.conditions input {
  margin: 0 10px 0 0;
  height: 16px;
}
.conditions a {
  color: #00B4C4;
}
.signupBtn {
  float: right;
  padding: 10px 30px;
  width: 455px;
}
.span7.signupDiv {
  position: relative;
  left: 25%;
  margin: 20px 20px 50px 20px;
  padding-top: 25px;
}  
.signupDiv p {
  position: relative;
  left: 35%;
  padding-bottom: 5%;
  font-size: 24px;
  color: #fff;  
}
.logged-out .col-two {
  border: none;
}
div.other-stories.span3 {
  width: 19%;
}
div.other-header {
  background: rgba(0, 0, 0, 0.7);
  color: #FFF;
  padding: 12px 8px;
  border-radius: 6px 6px 0 0;
  -webkit-border-radius: 6px 6px 0 0;
  -moz-border-radius: 6px 6px 0 0;
  -ms-border-radius: 6px 6px 0 0;
}
.other-header h3 {
  font-size: 16px;
  line-height: 24px;
}
ul.other-stories {
  padding: 10px;
  background: #E9E8E8;
  border-radius: 0 0 6px 6px;
  -webkit-border-radius: 0 0 6px 6px;
  -moz-border-radius: 0 0 6px 6px;
  -ms-border-radius: 0 0 6px 6px;
}
li.other-story {
  border-bottom: 1px solid #000;
  padding-bottom: 10px;
  padding-top: 8px;  
}
li.other-story:last-child {
  border-bottom: none;
}
.other-story .story-text {
  padding: 0;
  margin-bottom: 10px;
  word-wrap: break-word;
  width: 100%;
  max-height: 80px;
  overflow: hidden;
}
.other-story ul.story-tags {
  padding-left: 1px;
}
.other-story .like-area {
  padding: 0 0 5px 0;
  background: #E9E8E8;
}
#validation-errors {
  background: rgb(255, 240, 160);
  border-radius: 4px;
  display: none;
  float: right;
  margin: 0 0 20px;
  padding: 10px;
  width: 435px;
  transition: 0.5s ease-in;
  transition-delay: 1s;
}
form label.error {
  color: #FF5200;
  font-style: italic;
  padding-top: 3px;
}


/* Modal for login */
#ls-modal {
  background: #E9E8E8;
  width: 70%;
  left: 36%;
}
#ls-modal .modal-header {
  background: #333;
  color: #fff;
}
#ls-modal .modal-header a.close {
  color: #fff;
  border: 1px solid #fff;
  border-radius: 50%;
  padding: 3px;
}
#ls-modal .signup-form h3 {
  border-bottom: 1px solid #000;
}
#ls-modal form label.error {
  font-size: 12px;
  padding: 0;
}
.ls-container {
  position: relative;
  color: #000;
}
.l-container {
  float: left;
  border-right: 1px solid #aaa;
}
.s-container {
  float: right;
}
.ls-container form input[type=text],
.ls-container form input[type=password] {
  width: 193px;
}
.ls-container .conditions label,
.ls-container .signup-form h3 {
  color: #000;
}
.ls-container .conditions a {
  color: #005580;
}
.l-container button.signin {
  width: 455px;
  padding: 10px 30px;
  font-size: 15px;
  position: relative;
  left: 3%;
  margin-top: 15px;
}
.l-container .control-group {
  margin-bottom: 0;
}
.l-container .controls a {
  margin-left: 20px;
}
.s-container .conditions label {
  font-family: 'lucida_sansregular', Arial, Helvetica, sans-serif;
  font-size: 16px;
}
.ls-container .login-error {
  background: #f2dede;
  border: 1px solid #eed3d7;
  color: #b94a48;
  padding: 10px;
  margin-bottom: 10px;
  border-radius: 4px;
  overflow: hidden;
}
