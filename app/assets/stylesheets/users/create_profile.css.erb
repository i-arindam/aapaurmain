body {
  background: url('../web-texture-blue.jpg');
}

.qq-upload-list {
  margin:0;
}

#dob .btn-group {
  display: inline;
}

#dob .btn-group a.dropdown-toggle {
  margin: 0 10px 0 0;
}

.span10 .span9 {
  height: 30px;
  margin-bottom: 10px;
  padding-top: 9px;
  font-size: 16px;
  width: 90%;
}

[class*=more]{
  margin-top: 10px;
}

[class*=more] i {
  padding-right: 5px;
}

.help-inline {
  font-size: 11px;
  color: gray;
}

button.saveDetails {
  position: relative;
  left: 40%;
}

.profileContainer {
  background: rgba(0, 255, 214, 0.28);
  border-radius: 10px;
  -moz-border-radius: 10px;
  -ms-border-radius: 10px;
  position: relative;
  left: 18%;
  width: 60%;
  top: -260px;
}

.profileContainer form {
  padding-top: 20px;
  color: white;
}

.control-label {
  font-family: 'Oregano' cursive;
  font-size: 18px;
  font-weight: 700;
  width: 230px;
  margin-right: 20px;
}

div.control-group {
  position: relative;
  left: 80px;
}

label.control-label.wider {
  width: 250px;
  position: relative;
  left: -110px;
}

label.control-label.wider + div.controls {
  position: relative;
  left: -110px;
}

span.help-inline {
  color: #FFD700;
}

form label.error {
  color: #000;
  font-style: normal;
  padding-top: 3px;
  font-weight: bolder;
}

label.control-label.wider + div.controls label.error {
  margin-left: 110px;
}

div.alert.alert-error {
  top: -110px;
  left: 20%;
  width: 55%;
}

strong.must {
  margin-left: 6px;
  color: red;
  font-size: 24px;
}

.upload{
  color:white;
}

.picUpload {
  position: relative;
  top: 50px;
  left: 20px;
  margin-right: 20px;
  /*padding: 20px 20px 10px 20px;*/
 /* background: rgba(0, 255, 214, 0.28);*/
  border-radius: 10px;
  -moz-border-radius: 10px;
  -ms-border-radius: 10px;
  width: 15%;
}

