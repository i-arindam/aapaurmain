
/* Profile individual */
.individual-pro {
  border: 1px solid #9D9D99;
  position: relative;
  border-radius: 5px;
  box-shadow: 0 0 15px 0 #B7B5B6;
  padding: 15px;
  background: #F0F0F0;
  margin: 61px 0 55px;
}
.user-box img {
  border-radius: 5px;
  -webkit-border-radius: 5px;
  -moz-border-radius: 5px;
  -ms-border-radius: 5px;
  -o-border-radius: 5px;
  box-shadow: 0 0 4px rgba(0, 0, 0, .5);  
  -webkit-box-shadow: 0 0 4px rgba(0, 0, 0, .5);
  -moz-box-shadow: 0 0 4px rgba(0, 0, 0, .5);  
}
.box-rt {
  width: 33%;
  float: left;
}
.blg-tit {
  font-size: 14px;
  color: #4e4b4a;
}
.blg-ct {
  color: #7c7c7c;
  font-size: 13px;
  padding-top: 10px;
}
.basic-details {
  padding-top: 20px;
}
.blog-lt h3 {
  color: #4E4B4A;
  font-family: 'lucida_sansdemibold_roman';
  font-size: 14px;
  text-transform: uppercase;
}
.blog-lt ul li {
  border-bottom: 1px solid #CCCCC6;
  padding: 7px 4px;
}
.blog-lt ul li.last {
  border: medium none;
}
/* Profile individual ends */
.star {
  position: relative;
  left: 53%;
  top: 10px;
  display: none;
}
.rate-hint {
  position: relative;
  left: 55%;
  top: 10px;
  display: none;
}
.ratings {
  position: relative;
  top: 7px;
  font-size: 14px;
  display: none;
}
.star img{
  width: 28px;
  height: 28px;
  display: inline;
}
.individual-pro .user-inform p {
  color: #000;
}
