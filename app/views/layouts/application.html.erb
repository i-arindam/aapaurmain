<!DOCTYPE html>
<html>
  <head>
    <%= stylesheet_link_tag    "all/bootstrap.min" %>
    <%= stylesheet_link_tag    "application-all", media: "all" %>
    <%= javascript_include_tag "application-all" %>
    <%= csrf_meta_tags %>
    <%= render 'layouts/shim' %>    
    <%= yield :stylesheet %>
    <%= yield :javascript %>
  </head>
  <body>
    <%= render 'layouts/header' %>
  <div class = "wrapper">
    
    <% flash.each do |name, msg| %>
      <%= content_tag :div, msg, :id => "flash_#{name}" %>
    <% end %>
    <%= yield %>
  <div class="push"></div>
  </div>
    <%= yield :js_footer %>
    <%= render 'layouts/footer' %>
  </body>
</html>